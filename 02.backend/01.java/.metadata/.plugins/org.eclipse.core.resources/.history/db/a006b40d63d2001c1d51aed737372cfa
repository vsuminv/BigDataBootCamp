package com.builder;

public class Mouse {
	private String name;
	private String address;
	private String color;
	private int age;
	private int heigth;
	
	public Mouse(String name, String address, String color, int age, int heigth) {
		super();
		this.name = name;
		this.address = address;
		this.color = color;
		this.age = age;
		this.heigth = heigth;
	}
	
	

}
